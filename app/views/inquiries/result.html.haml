%div.result
  %h1= @safety_display

  %h2 Lon: #{@inquiry.lon} Lat: #{@inquiry.lat}
  = link_to "Google Maps", "http://maps.google.com/maps?z=17&t=m&q=loc:#{@inquiry.lon}+#{@inquiry.lat}"

  %ul#reasons
    - @reasons.each do |r|
      /= r
      - unless r.nil?
        %li.reason
          = r.text
